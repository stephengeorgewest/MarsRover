package rover;

import java.awt.Image;

public class Bitmap {

	byte[] rgb = null;
	int width = -1;
	int height = -1;
	
	public Bitmap(int width, int height){
		this.width = width;
		this.height = height;
		rgb = new byte[width*height*3];
	}
	
	public Image getImage(){
		
		
		return null;
	}
	
	
}
